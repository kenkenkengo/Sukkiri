<% if post.liked_by(current_user).present? %>
  <%= link_to image_tag("liked-parts.png", size: "30x30"), group_post_like_path(group, post, post.liked_by(current_user)), method: :DELETE, remote: true %>
<% else %>
  <%= link_to image_tag("like-parts.png", size: "30x30"), group_post_likes_path(group, post), method: :POST, remote: true %>
<% end %>