<% provide(:title, "通知一覧") %>
<div class="container">
  <div class="row">
    <div class="col-md-12">
      <h3>通知一覧</h3>
    </div>
  </div>
  <% if @notifications.present? %>
    <%= will_paginate @notifications, previous_label: '<', next_label: '>' %>
      <ul class="notifications">
        <%= render @notifications %>
      </ul>
    <%= will_paginate @notifications, previous_label: '<', next_label: '>' %>
  <% end %>
</div>