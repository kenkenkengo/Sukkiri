<li id="like-<%= like.id %>">
  <div class="row">
    <div class="col-md-2">
      <%= link_to "#{like.post.group.name}", group_posts_path(like.post.group) %>
      <%= image_tag like.post.image.url %>
    </div>
    <div class="col-md-7">
      <p><%= like.post.content %></p>
    </div>
  </div>
</li>
